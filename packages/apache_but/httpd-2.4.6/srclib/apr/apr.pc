prefix=/opt/apache_but/httpd/
exec_prefix=/opt/apache_but/httpd/
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=/opt/apache_but/httpd//include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.4.8
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lrt -lcrypt  -lpthread -ldl
Cflags: -DLINUX -D_REENTRANT -D_GNU_SOURCE -D_LARGEFILE64_SOURCE  -pthread -I${includedir}
