# This is the basic Auth Bruteforce demo

misc-settings
set maxThreads 10
back

plugins
audit htaccessMethods
bruteforce basicAuthBrute
discovery allowedMethods
grep httpAuthDetect,passwordProfiling
output console,textFile
output config textFile
set fileName output-w3af.txt
set verbose True
back
output config console
set verbose False
back
back

target
set target http://localhost/w3af/bruteforce/basicAuthBrute/easy_guess/
back

start

assert len(kb.kb.getData('basicAuthBrute','auth')) == 1

exit
