include("revisions-lib.inc");
tag_summary = "Runs a console command on the Lotus Domino Console using the given authentication credentials (see
also: domcon-brute)


SYNTAX:

domcon-cmd.pass:  The password used to authenticate to the server



domcon-cmd.cmd:  The command to run on the remote server


domcon-cmd.user:  The user used to authenticate to the server";

if(description)
{
    script_id(104099);
    script_version("$Revision: 10 $");
    script_tag(name:"last_modification", value:"$Date: 2013-10-27 11:03:59 +0100 (So, 27. Okt 2013) $");
    script_tag(name:"creation_date", value:"2011-06-01 16:32:46 +0200 (Wed, 01 Jun 2011)");
    script_tag(name:"cvss_base", value:"6.5");
    script_tag(name:"cvss_base_vector", value:"AV:N/AC:L/Au:S/C:P/I:P/A:P");
    script_tag(name:"risk_factor", value:"High");
    script_name("Nmap NSE net: domcon-cmd");
    desc = "
    Summary:
    " + tag_summary;

    script_description(desc);

    script_summary("Nmap NSE net: domcon-cmd");
    script_category(ACT_INIT);
    script_copyright("NSE-Script: The Nmap Security Scanner; NASL-Wrapper: Greenbone Networks GmbH");
    script_family("Nmap NSE net");

    script_add_preference(name:"domcon-cmd.pass", value:"", type:"entry");
    script_add_preference(name:"domcon-cmd.cmd", value:"", type:"entry");
    script_add_preference(name:"domcon-cmd.user", value:"", type:"entry");

    if (revcomp(a: OPENVAS_VERSION, b: "6.0+beta5") >= 0) {
      script_tag(name : "summary" , value : tag_summary);
    }
    exit(0);
}


include("nmap.inc");

# The corresponding NSE script does't belong to the 'safe' category
if (safe_checks()) exit(0);

phase = 0;
if (defined_func("scan_phase")) {
    phase = scan_phase();
}

if (phase == 1) {
    # Get the preferences
    argv = make_array();

    pref = script_get_preference("domcon-cmd.pass");
    if (!isnull(pref) && pref != "") {
        argv["domcon-cmd.pass"] = string('"', pref, '"');
    }
    pref = script_get_preference("domcon-cmd.cmd");
    if (!isnull(pref) && pref != "") {
        argv["domcon-cmd.cmd"] = string('"', pref, '"');
    }
    pref = script_get_preference("domcon-cmd.user");
    if (!isnull(pref) && pref != "") {
        argv["domcon-cmd.user"] = string('"', pref, '"');
    }
    nmap_nse_register(script:"domcon-cmd", args:argv);
} else if (phase == 2) {
    res = nmap_nse_get_results(script:"domcon-cmd");
    foreach portspec (keys(res)) {
        output_banner = 'Result found by Nmap Security Scanner (domcon-cmd.nse) http://nmap.org:\n\n';
        if (portspec == "0") {
            security_hole(data:output_banner + res[portspec], port:0);
        } else {
            v = split(portspec, sep:"/", keep:0);
            proto = v[0];
            port = v[1];
            security_hole(data:output_banner + res[portspec], port:port, protocol:proto);
        }
    }
}
