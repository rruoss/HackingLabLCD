<!DOCTYPE Menu PUBLIC "-//freedesktop//DTD Menu 1.0//EN"
  "http://www.freedesktop.org/standards/menu-spec/1.0/menu.dtd">
<Menu>
  <Name>Applications</Name>
  <Directory>X-GNOME-Menu-Applications.directory</Directory>
  <!-- Scan legacy dirs first, as later items take priority -->
  <LegacyDir>/etc/X11/applnk</LegacyDir>
  <LegacyDir>/usr/share/gnome/apps</LegacyDir>
  <!-- Read standard .directory and .desktop file locations -->
  <DefaultAppDirs />
  <DefaultDirectoryDirs />
  <!-- Read in overrides and child menus from applications-merged/ -->
  <DefaultMergeDirs />

  <!-- System Tools-->
  <Menu>
    <Name>System</Name>
    <Directory>System-Tools.directory</Directory>
    <Include>
      <And>
        <Category>System</Category>
        <Not>
          <Category>Settings</Category>
        </Not>
        <Not>
          <Category>Game</Category>
        </Not>
      </And>
    </Include>
    <Menu>
      <Name>Preferences</Name>
      <Directory>Settings.directory</Directory>
      <Include>
        <And>
          <Category>Settings</Category>
          <Not>
            <Or>
              <Category>System</Category>
              <Category>X-GNOME-Settings-Panel</Category>
            </Or>
          </Not>
        </And>
      </Include>
    </Menu>
    <Menu>
      <Name>Administration</Name>
      <Directory>Settings-System.directory</Directory>
      <Include>
        <And>
          <Category>Settings</Category>
          <Category>System</Category>
          <Not>
            <Category>X-GNOME-Settings-Panel</Category>
          </Not>
        </And>
      </Include>
    </Menu>
  </Menu>

  <!-- Accessoires -->
  <Menu>
    <Name>Accessories</Name>
    <Directory>Utility.directory</Directory>
    <Include>
      <And>
        <Category>Utility</Category>
          <Not>
      <Category>Accessibility</Category>
      </Not>
      <Not>
        <Category>System</Category>
      </Not>
    </And>
    </Include>
  </Menu>

  <!-- HackingLab  -->
  <Menu>
    <Name>HackingLab</Name>
    <Include>
      <And>
        <Category>X-HackingLab</Category>
      </And>
    </Include>
    <Menu>
      <Name>Landing Page</Name>
      <Include>
        <And>
          <Category>X-LandingPage</Category>
        </And>
      </Include>
    </Menu>
  </Menu>

  <!-- Malware -->
  <Menu>
    <Name>Malware</Name>
    <Include>
      <And>
        <Category>Malware</Category>
      </And>
    </Include>
  </Menu>

  <!-- Encryption -->
  <Menu>
    <Name>Encryption</Name>
    <Include>
      <And>
        <Category>Encryption</Category>
      </And>
    </Include>
  </Menu>  

  <!-- Database -->
  <Menu>
    <Name>Database</Name>
    <Include>
      <And>
        <Category>Database</Category>
      </And>
    </Include>
  </Menu>

  <!-- Networking -->
  <Menu>
    <Name>Networking</Name>
    <Directory>Networking.directory</Directory>
    <Include>
      <And>
        <Category>X-Networking</Category>
      </And>
    </Include>
    <Menu>
      <Name>Browsers</Name>
      <Directory>Browsers.directory</Directory>
      <Include>
        <And>
          <Category>WebBrowser</Category>
        </And>
      </Include>      
    </Menu>
    <Menu>
      <Name>Remote Desktop</Name>
      <Include>
        <And>
          <Category>X-RemoteDesktop</Category>
        </And>
      </Include>      
    </Menu>    
  </Menu>
  <Menu>
    <Name>Web Security</Name>
    <Directory>WebSec.directory</Directory>
    <Include>
      <And>
        <Category>WebSec</Category>
      </And>
    </Include>
  </Menu>

    <!-- Other (aka standard menu)-->
    <Menu>
      <Name>Other</Name>
      <Directory>X-GNOME-Other.directory</Directory>
<!-- Accessibility submenu -->
<Menu>
  <Name>Universal Access</Name>
  <Directory>Utility-Accessibility.directory</Directory>
  <Include>
    <And>
      <Category>Accessibility</Category>
      <Not>
        <Category>Settings</Category>
      </Not>
    </And>
  </Include>
</Menu>
<!-- End Accessibility -->
<!-- Development Tools -->
<Menu>
  <Name>Development</Name>
  <Directory>Development.directory</Directory>
  <Include>
    <And>
      <Category>Development</Category>
    </And>
    <Filename>emacs.desktop</Filename>
  </Include>
</Menu>
<!-- End Development Tools -->
<!-- Education -->
<Menu>
