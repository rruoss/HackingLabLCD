      </Not>
      <Not>
        <Category>SportsGame</Category>
      </Not>
      <Not>
        <Category>StrategyGame</Category>
      </Not>
    </And>
  </Include>
  <DefaultLayout inline="true" inline_limit="6" inline_header="false">
    <Merge type="menus" />
    <Merge type="files" />
  </DefaultLayout>
