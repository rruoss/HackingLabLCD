<!-- End Science -->
<!-- Games -->
<Menu>
  <Name>Games</Name>
  <Directory>Game.directory</Directory>
  <Include>
    <And>
      <Category>Game</Category>
      <Not>
        <Category>ActionGame</Category>
      </Not>
      <Not>
        <Category>AdventureGame</Category>
      </Not>
      <Not>
        <Category>ArcadeGame</Category>
      </Not>
      <Not>
